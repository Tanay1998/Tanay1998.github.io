			<script language="JavaScript">
				function Search_newfn_1(typesearch) 
				{    
					if (typesearch == 1) 
					{        
						var hidVal = document.frm_searchweb.SearchboxChoosedVal.value;   
						
						if (hidVal == 0) 
						{      
							searchParam = 'search'        
						} 
						else if (hidVal == 1) 
						{ 
							searchParam = 'Searchbox_Google'        
						} 
						dd = eval('document.frm_searchweb.' + searchParam);
						if (dd.value == '' || dd.value == 'Search...') 
						{ 
							alert('Please enter any valid search parameter.');
							dd.focus() 
						}
						else 
						{
							if(document.getElementById('search_sify').checked)		{	
								var searchstr= dd.value;
								var searchstrArray = searchstr.split(' ');
								for(var i=0;i<searchstr.length;i++){
									searchstr = searchstr.replace(' ','_');	
								}		
								//window.location.href='http://www.sify.com/topics/'+searchstr+'.html';	
								window.open('http://www.sify.com/topics/'+searchstr+'.html');
							}	
							else if(document.getElementById('search_web').checked)		{						
								sitesearch = document.getElementById('sitesearch').value;
								ss = '32&amp;rd=' + (sitesearch == '1' ? 'sify' : 'web');
								document.frm_searchweb.action = 'http://search.sify.com/search.php?q=' + encodeURIComponent(dd.value) + '&amp;ss=' + ss;            document.frm_searchweb.search.value = '';            
								document.frm_searchweb.submit()	
							}   
						}    
					}
				}
			</script>
			    <div itemtype="http://schema.org/WebSite" itemscope="">
				    <meta content="http://www.sify.com/" itemprop="url">
				    <form class="form-wrapper col-md-12 col-xs-12 nopadding" itemtype="http://schema.org/Read" itemscope="" itemprop="potentialAction" style="display:inline; width:100%;" target="_new" onsubmit="Search_newfn_1(1);return false;" method="post" name="frm_searchweb">
					<meta content="http://www.sify.com/topics/{search}.html" itemprop="target">
