function validate_signup_form() {
//alert('hello');
var x = document.getElementById("signup_email");
//alert(x);
    if (x.value == null | x.value == '') {
       // x.style.borderColor = "red";
        alert("Email must be filled out");
        return false;
    }
var x = document.getElementById("signup_password");
    if (x.value == null | x.value == '') {
      //  x.style.borderColor = "red";
       alert("Password must be filled out");
        return false;
    }
var x = document.getElementById("signup_select");
    if (x.value == null | x.value == '') {
//        x.style.borderColor = "red";
       alert("Select the option to prepare");
        return false;
    } 
}

function validate_login_form() {
//alert('hello');
var x = document.getElementById("login_email");
//alert(x);
    if (x.value == null | x.value == '') {
        x.style.borderColor = "red";
       // alert("Email must be filled out");
        return false;
    }
var x = document.getElementById("login_password");
    if (x.value == null | x.value == '') {
        x.style.borderColor = "red";
     //  alert("select must be filled out");
        return false;
    }
}

function validate_forgot_form() {
//alert('hello');
var x = document.getElementById("forgot_email");
//alert(x);
    if (x.value == null | x.value == '') {
        x.style.borderColor = "red";
       // alert("Email must be filled out");
        return false;
    }
}

