		analytics.load("YpVKt06uTKOtczMtz8oyfahcrzPr5ees");
		//analytics.page()
	}}();
	
	function segmentTrack(obj, details_obj){
	    var fold = null;
	    if(obj){
	      var top = (jQuery(obj).offset()) ? jQuery(obj).offset().top : 0;
	      fold = parseInt(top / jQuery(window).height()) + 1;
	    }
	    var isResponsive = (jQuery(window).width() <= 1024);
	    var isDesktop = (jQuery(window).width() > 1024);
	    var url = window.location.href;
	    var origin = window.location.origin;
	    var current_time = new Date();
	    var action_grammar = {
	        'click': "Clicked",
	        'mouseenter': "Hovered over"
	    }

	    details_obj['cta_type'] = action_grammar[details_obj['action']] + " " + details_obj['cta_text'];
	    details_obj['log_type'] = 'landing_page';
	    details_obj['environemnt'] = "";
	    details_obj['fold'] = fold;
	    details_obj['isResponsive'] = isResponsive;
	    details_obj['isDesktop'] = isDesktop;
	    details_obj['page_url'] = url;
	    details_obj['timestamp'] = current_time;
	    details_obj['cookie_info'] = document.cookie;

	    analytics.track(details_obj.cta_type,details_obj);
	}
	
	window.onload = function(){
		var segmentElements = document.getElementsByClassName("segmentTrack");
		var segmentObjects = {};

		for (var i = 0, n = segmentElements.length; i < n; ++i) {
	    	var el = segmentElements[i];

		    var obj = {};
		    if(!el.id){ el.setAttribute('id', 'segmentTrackID'+i); }
		    obj['id'] = el.id;
		    obj['cta_text'] = el.innerText;
		    if(el.href){ obj['destination_url'] = el.href; }

	   		// console.log(i);

		    elemAttributes = el.attributes
		    for(var attr, attrName, j = 0; j < elemAttributes.length; j++){
				attr = elemAttributes[j];
				if(attr.nodeName.match(/data-segment-.+/)){
					attrName = attr.nodeName.split('data-segment-')[1].replace('--','_');
					obj[attrName] = attr.nodeValue;
				}
		    }

		    segmentObjects[el.id] = obj;

		    segmentElements[i].addEventListener(obj['action'], function(e){
				var elemId;
				for(var k=0; k< e.path.length; k++){
					if(e.path[k].className.match(/segmentTrack/)){
					  elemId = e.path[k].id;
					  break;
					}
				}
				// console.log(e);
				// console.log(obj);
				obj = document.getElementById(obj['id']);
				obj_details = segmentObjects[elemId];
				obj_details['time_after_visit'] = e.timeStamp;
				segmentTrack(obj, obj_details);
		    });
	  	}

	  window.segmentElements = segmentObjects;
	}
}
</script>
