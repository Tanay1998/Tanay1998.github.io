    <script type="text/javascript">
        function changePanel(cid, pId, flag) {
            //alert(cid);                    
            menuService.changeLeadNews(cid, pId, flag, onSucceeded1, onFailed1);
            return true;
        }
        function onFailed1(error) {
            //alert(error.get_message());
            return false;
        }
        function onSucceeded1(result) {
            //alert(result);
            var strVal = result.split('^');

            document.getElementById('showPanel' + strVal[1]).innerHTML = strVal[0];
            return false;
        }
    </script>

